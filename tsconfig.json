{
  "compilerOptions": {
	"module": "commonjs",
	"target": "es5",
	"noImplicitAny": true,
	"sourceMap": true,
	"moduleResolution": "node",
	"rootDir": "src",
	"outDir": "dist",
	"jsx": "react",
	"typeRoots": [
	  "./src/@types", "node_modules/@types"]
  },
  "include": [
	"src/**/*"
  ],
  "exclude": [
	"node_modules"
  ]
}